    <param name="obstacle1" command="$(find xacro)/xacro '$(find limo_challenges)/urdf/obstacle.xacro' " />
    <node name="urdf_spawner_obstacle1" pkg="gazebo_ros" type="spawn_model" args="-urdf -model obstacle1 -x 3.0 -y 4.2 -z 0.2 -Y 0.5 -param obstacle1"/>      
    
    <param name="obstacle2" command="$(find xacro)/xacro '$(find limo_challenges)/urdf/obstacle.xacro' " />
